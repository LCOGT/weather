{
  "_source": {
    "excludes": [],
    "includes": [
      "site",
      "timestampmeasured"
    ]
  },
  "aggregations": {
    "date(timestampmeasured,30m)": {
      "aggregations": {
        "avg(value_float)": {
          "avg": {
            "field": "value_float"
          }
        }
      },
      "date_histogram": {
        "field": "timestampmeasured",
        "format": "yyyy/MM/dd HH:mm:ss",
        "interval": "30m",
        "min_doc_count": 1
      }
    }
  },
  "query": {
    "bool": {
      "filter": {
        "bool": {
          "must": [
            {
              "range": {
                "timestampmeasured": {
                  "gt": "{{.Start}}"
                }
              }
            },
            {
              "range": {
                "timestampmeasured": {
                  "lte": "{{.End}}"
                }
              }
            },
            {
              "term": {
                "datumname": "{{.DatumName}}"
              }
            },
            {
              "term": {
                "site": "{{.Site}}"
              }
            }
          ]
        }
      }
    }
  },
  "size": 0
}
