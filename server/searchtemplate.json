{
  "_source": {
    "excludes": [],
    "includes": [
      "site",
      "timestampmeasured",
      "value_string",
      "value_float"
    ]
  },
  "from" : 0,
  "size" : 10000,
  "sort": [
    {
      "timestampmeasured": {
        "order": "asc"
      }
    }
  ],
  "query": {
    "bool": {
      "filter": {
        "bool": {
          "must": [
            {
              "range": {
                "timestampmeasured": {
                  "gt": "{{.Start}}"
                }
              }
            },
            {
              "range": {
                "timestampmeasured": {
                  "lte": "{{.End}}"
                }
              }
            },
            {
              "term": {
                "datumname": "{{.DatumName}}"
              }
            },
            {
              "term": {
                "site": "{{.Site}}"
              }
            },
            {
              "term": {
                "datuminstance": "1"
              }
            }
          ]
        }
      }
    }
  }
}
